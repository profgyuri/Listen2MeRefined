<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#00132c</Color><!-- 60% -->
    <Color x:Key="SecondaryColor">#13d5fa</Color><!-- 30% -->
    <Color x:Key="ThirnaryColor">#86f1e4</Color><!-- 10% -->
    <Color x:Key="ThirnaryMidColor">#66b1c4</Color>
    <Color x:Key="ThirnaryDarkColor">#4691a4</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="ThirnaryBrush" Color="{StaticResource ThirnaryColor}"/>
    <SolidColorBrush x:Key="ThirnaryMidBrush" Color="{StaticResource ThirnaryMidColor}"/>
    <SolidColorBrush x:Key="ThirnaryDarkBrush" Color="{StaticResource ThirnaryDarkColor}"/>
</ResourceDictionary>