<Window x:Class="Listen2MeRefined.WPF.Views.NewSongWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:vm="clr-namespace:Listen2MeRefined.Infrastructure.Mvvm;assembly=Listen2MeRefined.Infrastructure"
        xmlns:page="clr-namespace:Listen2MeRefined.WPF.Views.Pages"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance vm:NewSongWindowViewModel}"
        Style="{StaticResource BasicWindow}"
        Topmost="True"
        ResizeMode="NoResize"
        FontSize="18"
        Title="NewSongWindow" Height="90" Width="450">

    <Border
        BorderBrush="{DynamicResource SecondaryBrush}"
        BorderThickness="1"
        CornerRadius="3">
        <Frame
            x:Name="ContentPage"
            NavigationUIVisibility="Hidden"/>
    </Border>
</Window>